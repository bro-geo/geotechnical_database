-- Table: ext.grid_sicad_1000

-- DROP TABLE ext.grid_sicad_1000;

CREATE TABLE ext.grid_sicad_1000
(
    gid integer NOT NULL DEFAULT nextval('ext."GRID_SICAD_1000_gid_seq"'::regclass),
    grid_leg character varying(50) COLLATE pg_catalog."default",
    grid_area numeric,
    grid_lengt numeric,
    ic_seq bigint,
    geom geometry(Polygon,31983),
    CONSTRAINT "GRID_SICAD_1000_pkey" PRIMARY KEY (gid)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE ext.grid_sicad_1000
    OWNER to ext;

GRANT SELECT ON TABLE ext.grid_sicad_1000 TO consulta;

GRANT ALL ON TABLE ext.grid_sicad_1000 TO ext;

GRANT SELECT, UPDATE ON TABLE ext.grid_sicad_1000 TO ic;

GRANT ALL ON TABLE ext.grid_sicad_1000 TO sde;

-- Index: GRID_SICAD_1000_geom_idx

-- DROP INDEX ext."GRID_SICAD_1000_geom_idx";

CREATE INDEX "GRID_SICAD_1000_geom_idx"
    ON ext.grid_sicad_1000 USING gist
    (geom)
    TABLESPACE pg_default;
