# sde user
CREATE USER sde LOGIN CREATEDB CREATEROLE; 
CREATE SCHEMA AUTHORIZATION sde;
ALTER USER sde WITH PASSWORD '$3cr3t';
ALTER DEFAULT PRIVILEGES IN SCHEMA sde GRANT ALL ON tables TO sde;

# ic user 
CREATE USER ic LOGIN;
CREATE SCHEMA AUTHORIZATION ic;
ALTER USER ic WITH PASSWORD '$3cr3t1';
ALTER DEFAULT PRIVILEGES IN SCHEMA ic GRANT ALL ON tables TO ic;

# dom user 
CREATE USER dom LOGIN;
CREATE SCHEMA AUTHORIZATION dom;
ALTER USER dom WITH PASSWORD '$3cr3t3';
ALTER DEFAULT PRIVILEGES IN SCHEMA dom GRANT ALL ON tables TO dom;

