# consulta user 
CREATE USER consulta LOGIN;
ALTER USER consulta WITH PASSWORD '$3cr3t9';
ALTER DEFAULT PRIVILEGES IN SCHEMA dom GRANT ALL ON tables TO consulta;
ALTER DEFAULT PRIVILEGES IN SCHEMA ic GRANT ALL ON tables TO consulta;
ALTER DEFAULT PRIVILEGES IN SCHEMA el GRANT ALL ON tables TO consulta;
ALTER DEFAULT PRIVILEGES IN SCHEMA cd GRANT ALL ON tables TO consulta;
ALTER DEFAULT PRIVILEGES IN SCHEMA md GRANT ALL ON tables TO consulta;
ALTER DEFAULT PRIVILEGES IN SCHEMA ext GRANT ALL ON tables TO consulta;
ALTER DEFAULT PRIVILEGES IN SCHEMA ctm GRANT ALL ON tables TO consulta;
